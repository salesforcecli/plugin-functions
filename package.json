{
  "name": "sf-plugin-functions",
  "description": "Functions plugin for the SF CLI",
  "version": "0.0.0",
  "author": "Salesforce, Inc.",
  "bugs": "https://github.com/heroku/sf-plugin-functions/issues",
  "dependencies": {
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "axios": "^0.21.1",
    "axios-debug-log": "^0.8.2",
    "netrc-parser": "^3.1.6",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "chai": "^4",
    "eslint": "^7.21.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.2",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/heroku/sf-plugin-functions",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/sf-plugin-functions",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
