    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.8",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.10",
    "@types/nodegit": "^0.26.15",
    "@types/sha256-file": "^1.0.0",
    "@types/sinon-chai": "^3.2.5",
