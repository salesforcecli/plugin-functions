    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-not-found": "^1.2.4",
    "@salesforce/core": "^2.20.5",
    "@salesforce/plugin-org": "^1.6.2",
    "@salesforce/templates": "^51.3.0",
    "@salesforce/ts-sinon": "^1.3.12",
    "@typescript-eslint/parser": "^4.24.0",
    "axios": "^0.21.1",
